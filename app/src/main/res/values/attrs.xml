<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SteamGridLayout">
        <!--画中画模式-->
        <attr name="sgl_pip_mode" format="boolean" />
        <!--画中画view的宽高占父布局的宽高比-->
        <attr name="sgl_pip_percent" format="float" />
        <attr name="sgl_pip_margin_top" format="dimension" />
        <attr name="sgl_pip_margin_end" format="dimension" />

    </declare-styleable>

    <declare-styleable name="CallLayout">
        <!--SteamGridLayout画中画模式-->
        <attr name="cl_sgl_pip_mode" format="boolean" />
        <attr name="cl_sgl_pip_percent" format="float" />
        <attr name="cl_sgl_pip_margin_top" format="dimension" />
        <attr name="cl_sgl_pip_margin_end" format="dimension" />

        <attr name="cl_speaker_mute_on" format="boolean" />
        <attr name="cl_microphone_mute_on" format="boolean" />
        <attr name="cl_speakerphone_on" format="boolean" />

        <attr name="cl_into_room_type" format="enum">
            <enum name="actively_into_room" value="1" />
            <enum name="be_invited_into_room" value="2" />
            <enum name="directly_into_room" value="3" />
        </attr>

    </declare-styleable>

    <declare-styleable name="P2PCallLayout">

    </declare-styleable>
</resources>